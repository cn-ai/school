<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.accp.msgconvert.dao.IBillDao">

	<select id="queryList" resultType="bill">
		select * from bill
	</select>
	
	<delete id="delBill">
		delete from bill where billno=#{billno}
	</delete>
</mapper>